
#include <conio.h>

int recep1;
int recep2;

int addit(int addend1, int addend2)
{
    return addend1+addend2;
}

int main()
{
    printf("Number 1\n");
    scanf("%i",&recep1);
    printf("Numb 2\n");
    scanf("%i",&recep2);
    printf(addit(recep1,recep2),"%i");
    return 0;
    system ("pause")
    
}
